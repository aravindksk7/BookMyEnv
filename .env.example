# BookMyEnv Environment Configuration
# Copy this file to .env and customize the values
# IMPORTANT: Never commit .env file to version control!

# Database Configuration
POSTGRES_USER=tem_user
POSTGRES_PASSWORD=CHANGE_THIS_TO_STRONG_PASSWORD_MIN_32_CHARS
POSTGRES_DB=tem_db

# JWT Configuration
# Generate a secure secret: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=CHANGE_THIS_TO_SECURE_RANDOM_STRING_MIN_64_CHARS_USE_CRYPTO_RANDOM

# Application Settings
NODE_ENV=production
PORT=5000

# CORS Settings
CORS_ORIGIN=https://yourdomain.com
FRONTEND_URL=https://yourdomain.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
AUTH_RATE_LIMIT_MAX=5
